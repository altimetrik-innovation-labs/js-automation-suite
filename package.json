{
  "name": "js-automation-suite",
  "version": "1.0.0",
  "description": "Base e2e test automation codebase built on GhostJS, Mocha and Chai. Beautifully crafted using ES2015",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:phantom && npm run test:slimerjs",
    "test:phantom": "ghostjs test/*.js",
    "test:slimerjs": "ghostjs --ghost-runner slimerjs test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/altimetrik-innovation-labs/js-automation-suite.git"
  },
  "keywords": [
    "ghostjs",
    "tests",
    "mocha",
    "es2015",
    "es7"
  ],
  "author": "Rodrigo Juliani <rjuliani@altimetrik.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/altimetrik-innovation-labs/js-automation-suite/issues"
  },
  "homepage": "https://github.com/altimetrik-innovation-labs/js-automation-suite#readme",
  "dependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "ghostjs": "^1.7.1",
    "slimerjs": "^0.10.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "standard": "^8.6.0"
  }
}
